<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>All Dogs</h1>

    <% for(i in dogs){%>
      <p>
        Name:<%= dogs[i].name %>
      </p>
      <p>
        Description:<%= dogs[i].description %>
      </p>
      <a href="/dogs/<%= dogs[i].id %>/edit"><button>Edit</button></a>
      <a href="/dogs/<%= dogs[i].id %>"><button>Show</button></a>
      <form class="" action="/dogs/<%= dogs[i].id %>/destroy" method="post">
        <button>Delete</button>
      </form>
      <!-- <a href="/dogs/<%= dogs[i].id %>/destroy" method='post'><button>Delete</button></a> -->
    <% } %>

  <br><a href="/dogs/new"><button>Add Dog</button></a>
  </body>
</html>
